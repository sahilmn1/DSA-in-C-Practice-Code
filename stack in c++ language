#include <iostream>
#include <stack>

using namespace std;

int
main ()
{
  stack < int >stack;
  stack.push (5);
  stack.push (4);
  stack.push (3);
  stack.push (2);
  stack.push (1);
  stack.pop ();

  while (!stack.empty ())
    {
      cout << stack.top ()<< " " ;
      stack.pop ();
    }
  cout << endl;

  cout << "Is this stack is empty ? : " << stack.size ();
  cout<<endl;
  cout<<stack.empty();
}
