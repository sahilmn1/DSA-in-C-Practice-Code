//here we will check whether the number is lies into the power of two or not print true or false 

#include <iostream>
#include <climits>
#include <math.h>
using namespace std;

int
main ()
{
  int n, ans;
  cout << "Enter the number to check : ";
  cin >> n;
  for (int i = 0; i <= 30; i++)
    {
      ans = pow (2, i);
      if (ans == n)
	{
	  cout << "True";
	  break;
	}

    }
  if (ans != n)
    {
      cout << "False";
    }


}
